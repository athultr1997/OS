#include <sys/types.h>
#include <stdio.h>
#include <unistd.h>

int main()
{
  pid_t pid;

  pid = fork();

  if(pid<0)
    {
      printf("Child was not creater. Error.\n");
    }  
  else if(pid==0)
    {
      printf("Child created successfully\n");
    }
  else if(pid>0)
    {
      printf("The parent process in running\n");
    }

  return 0;
}
